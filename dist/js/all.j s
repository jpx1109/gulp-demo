function move(r,n,a,i){if(r.timer)return!1;a=a||1e3;var l={};for(var e in n)l[e]=n[e];var o={};for(var e in n)o[e]=parseInt(getStyle(r)[e]);var u=new Date;r.timer=setInterval(function(){var e=(new Date-u)/a;for(var t in 1<e&&(e=1,clearInterval(r.timer),r.timer=null,i&&i()),n)r.style[t]=o[t]+(l[t]-o[t])*e+"px"},10)}function getStyle(e){return window.getComputedStyle?window.getComputedStyle(e,null):e.currentStyle}